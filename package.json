{
  "version": "1.0.0",
  "name": "aspnet",
  "private": true,
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "build:aot": "ngc -p src/tsconfig-aot.json && rollup -c /rollup-config.js",
    "build:aot-prod": "ngc -p src/tsconfig-prod-aot.json && rollup -c /rollup-config.js",
    "build:aot-watch": "ngc -p src/tsconfig-aot.json -w && rollup -c /rollup-config.js -w",
    "test": "concurrently \"npm run build:watch\" \"karma start \"karma.conf.js\"",
    "build:aot-cm-factory": "ngc -p /src/tsconfig-aot-cm.json",
    "build:aot-cm": "ngc -p src/tsconfig-aot-cm.json && rollup -c /rollup-config-cm.js",
    "build:combine": "npm run build:aot-cm & npm run build:aot"
  },
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.5",
    "@types/jasmine": "~2.5.43",
    "@types/node": "^6.0.63",
    "angular-in-memory-web-api": "^0.2.4",
    "bootstrap": "4.0.0-alpha.6",
    "clang-format": "~1.0.46",
    "core-js": "^2.4.1",
    "es6-promise": "~4.0.5",
    "es6-shim": "~0.35.3",
    "font-awesome": "^4.7.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.4.2",
    "systemjs": "^0.20.7",
    "uglify-js": "^2.8.7",
    "zone.js": "^0.8.14",
    "tether": "^1.4",
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "@types/jasmine": "~2.5.43",
    "@types/node": "^6.0.63",
