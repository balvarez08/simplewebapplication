    "gulp-exit": "~0.0.2",
    "gulp-minify-css": "~1.2.4",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "~3.1.0",
    "gulp-sourcemaps": "~2.4.1",
    "gulp-tsc": "~1.2.6",
    "gulp-typescript": "^3.1.4",
    "gulp-uglify": "~2.0.1",
    "gulp-util": "~3.0.8",
    "jasmine-core": "~2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "~0.2.2",
    "karma-phantomjs-launcher": "~1.0.2",
